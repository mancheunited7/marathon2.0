- if controller_name != 'sessions'
  = link_to "#{t '.login'}", new_session_path(resource_name),class: "btn btn-default btn-block fl_gh"
  %br/
- if devise_mapping.registerable? && controller_name != 'registrations'
  = link_to "#{t '.sign_up'}", new_user_registration_path(resource_name),class: "btn btn-default btn-block fl_gh"
  %br/
- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations'
  = link_to "#{t '.password_reset'}", new_password_path(resource_name),class: "btn btn-default btn-block "
  %br/
- if devise_mapping.confirmable? && controller_name != 'confirmations'
  = link_to "#{t '.resend_confirmation_instructions'}", new_confirmation_path(resource_name),class: "btn btn-default  btn-block"
  %br/
= link_to "twitter_login", user_twitter_omniauth_authorize_path, class: " btn btn-block fl_tw2 fa fa-twitter"
= link_to "facebook_login", user_facebook_omniauth_authorize_path, class: " btn btn-block fl_fb2 fa fa-facebook "
= link_to "google_login", user_google_omniauth_authorize_path, class: " btn btn-block fl_go2 fa fa-google "
= link_to "instagram_login", user_instagram_omniauth_authorize_path, class: " btn btn-block fl_ig2 fa fa-instagram "
